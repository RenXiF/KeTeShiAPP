  color: #fe2736;
  font-weight: bold;
}
.priceFontDiscount {
  color: #a1a1a1;
  text-decoration: line-through;
}
.noWrapF {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.centerlay {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
