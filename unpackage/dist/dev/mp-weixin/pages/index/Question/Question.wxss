.index_g {
  width: 100%;
  height: 100%;
}
.header {
  max-width: 100%;
  background-color: #F8F8F8;
  padding: 0 15px;
}
.header .tit_ck {
  height: 100%;
  padding: 5px 10px;
  border-bottom: #07D188 3px solid;
  font-size: 12px;
}
.foot {
  width: 100%;
  min-height: 700px;
  margin-top: 20px;
}
.foot .foot_card {
  max-width: 100%;
  color: #333333;
  background-color: #FFFFFF;
  margin-bottom: 30px;
}
.foot .foot_card .foot_tit,
.foot .foot_card .foot_content,
.foot .foot_card .foot_img {
  padding: 10px;
  border-bottom: #EEEEEE 1px solid;
}
.foot .foot_card .foot_tit {
  z-index: 999;
}
.foot .foot_card .foot_content {
  z-index: 999;
}
.foot .foot_card .foot_img image {
  border: #07D188 1px solid;
  max-width: 150px;
}

