.logoutb {
  width: 100%;
  padding: 5rpx 60rpx;
  background: none;
  /* box-shadow: 1px 2px 6px #F55866; */
  font-size: 38rpx;
  font-weight: 700;
}
.order_tit {
  max-width: 100%;
  min-height: 1200rpx;
  background-color: #F8F8F8;
}
.order_tit .button_ck {
  width: 100%;
  position: fixed;
  bottom: 0;
  z-index: 99;
  background: -webkit-linear-gradient(45deg, #00d099, #00c28b);
  background: linear-gradient(45deg, #00d099, #00c28b);
}
.order_tit .button_ck text {
  width: 100%;
  padding: 30rpx 0;
  color: #FFFFFF;
  font-size: 48rpx;
  font-weight: 700;
}
.order_tit .block_tit {
  max-width: 100%;
  min-height: 300rpx;
  margin: 30rpx;
  padding: 30rpx;
  background-color: #FFFFFF;
}
.order_tit .block_tit .name_tit {
  width: 100%;
  padding: 20rpx 0;
  font-size: 35rpx;
  color: #333333;
}
.order_tit .block_tit .name_ck {
  width: 100%;
  min-height: 150rpx;
}
.order_tit .block_tit .name_ck text {
  padding: 10rpx 0;
}

