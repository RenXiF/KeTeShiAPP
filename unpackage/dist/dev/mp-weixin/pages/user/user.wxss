}
.img_maxs {
	width: 30px;
	margin-right: 10px;
}
.logout {
	padding: 5px 10px;
	margin-right: 20px;
	color: #FFFFFF;
	/* box-shadow: 1px 2px 6px #F55866; */
	box-shadow: 1px 2px 6px #DAC1E7;
	/* background: linear-gradient(45deg,#c779d0,#4bc0c8); */
	background: -webkit-linear-gradient(45deg, rgba(254, 172, 94, 0.5), rgba(199, 121, 208, 0.5), rgba(75, 192, 200, 0.5));
